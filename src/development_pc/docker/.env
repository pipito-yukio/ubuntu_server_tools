#COMPOSE_PROJECT_NAME=postgres-16
HOST_PG_VOLUME=/home/yourname/db
# SynologyNAS: /volume1/Databases/postgres-16
# Host Nfs: /mnt/nas_databases <- SynologyNAS:/volume1/Databases
#HOST_PG_VOLUME=/mnt/nas_databases
DB_NAME=example_pgdb
PG_USER=postgres
PG_PASSWD=yourpostgrepasswd

# display locale
LANG=C

# locale
LANGUAGE=ja_JP:ja
LC_ALL=ja_JP.UTF-8
# Time zone
TZ=Asia/Tokyo

# volume mount
HOST_HOME=/home/yourname
CONTAINER_HOME=/home/yourname
